## install all updates in CLI

    apt-get update
    apt-get upgrade

## which provides

    pet@cow:~$ dpkg -S /bin/ls
    coreutils: /bin/ls
    #this is like rpm "which provides"

## search package

    aptitude search tig

## check which file to edit if you want a new theme    

    dpkg -l |grep lynx
    dpkg -L lynx-cur
    vim /etc/lynx-cur/lynx.cfg
    vim /etc/lynx-cur/lynx.lss
    sudo vim /etc/lynx-cur/lynx.lss
    lynx www.google.com

## dry-run

 sudo dpkg -i --dry-run php5-ffmpeg_0.6.0-1build2_amd64.deb

